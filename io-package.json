        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "info.deviceInfo",
      "type": "channel",
      "common": {
        "name": "Hardware Informationen"
      },
      "native": {}
    },
    {
      "_id": "info.deviceInfo.mac",
      "type": "state",
      "common": {
        "role": "state",
        "name": "mac",
        "type": "string",
        "read": true,
        "write": false
      },
      "native": {}
    },
    {
      "_id": "info.deviceInfo.front_sn",
      "type": "state",
      "common": {
        "role": "state",
        "name": "Serial# front",
        "type": "string",
        "read": true,
        "write": false
      },
      "native": {}
    },
    {
      "_id": "info.deviceInfo.puck_sn",
      "type": "state",
      "common": {
        "role": "state",
        "name": "Serial# puck",
        "type": "string",
        "read": true,
        "write": false
      },
      "native": {}
    },
    {
      "_id": "info.deviceInfo.details",
      "type": "state",
      "common": {
        "role": "json",
        "type": "string",
        "name": "Detailed informations",
        "read": true,
        "write": false
      },
      "native": {}
    },
    {
      "_id": "info.deviceInfo.dip_config",
      "type": "state",
      "common": {
        "role": "json",
        "type": "number",
        "name": "DIP Switches",
        "read": true,
        "write": false
      },
      "native": {}
    }
